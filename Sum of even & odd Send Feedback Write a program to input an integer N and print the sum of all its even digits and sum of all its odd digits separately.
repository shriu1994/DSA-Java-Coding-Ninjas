import java.util.*;
public class Main
{
   public static void main (String[] args) {
       Scanner sc = new Scanner(System.in);
       int n = sc.nextInt();
       int even = 0;
       int odd = 0;
       int res = 0;
       while (n > 0) {
            res =  n % 10;
             if(res%2==0){
                even = even +res;
            }else{
                odd= odd + res;
            }
            n = n / 10;
           
        }
        System.out.println(even+"   "+odd);
    }
    
}
