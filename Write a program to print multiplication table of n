import java.util.*;
public class Main
{
   public static void main (String[] args) {
       Scanner sc = new Scanner(System.in);
       int n = sc.nextInt();
       int i = 1;
       while(i<=10){
           System.out.println(n *i);
           i++;
       }
   

    }
    
}

Method 2 - using for loop
public class Main
{
   public static void main (String[] args) {
       Scanner sc = new Scanner(System.in);
       int n = sc.nextInt();
       for (int i=1; i<=10;i++){
           System.out.println(n*i);
       }
      
   

    }
    
}
